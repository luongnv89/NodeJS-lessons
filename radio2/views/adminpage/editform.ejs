<% if(success){%>
    <form role="form" action="<%=channel._id%>" method="post">
    <div class="form-group">
      <input class="form-control" type="name" name="rsName" placeholder="Radio stream name" value="<%=channel.name%>" required>
    </div>
    <div class="form-group">
      <input  class="form-control" type="text" name="rsTags" placeholder="Tags, divided by a comma" value="<%=channel.tags%>" required>
    </div>
    <div class="form-group">
      <textarea class="form-control" name="rsUrls" placeholder="Enter many stream url, divided by a comma" value="" required><%=channel.urls%></textarea>
    </div>
    <div class="form-group">
      <input  class="form-control" type="url" name="rsLogo" placeholder="Enter logo url" value="">
      <div>
        <input type="file" name="rslogoFile">
      </div>
    </div>
    <div class="form-group">
      <img alt="Logo preview" src="<%=channel.logo%>">
    </div>
    <button type="submit" class="btn btn-default">Update</button>
</form>
<% }else{%>
  <h2>Cannot found the radio stream</h2>
<% }%>
